#Classe veiculo com atributo velocidade_maxima com geter
class Veiculo():
    def __init__(self, velocidade_maxima):
     self.__velocidade_maxima= velocidade_maxima

    def get_velocidade_maxima (self):
        return self.__velocidade_maxima
    
carro1 = Veiculo(input('digite a velocidade maxima do carro em KM: '))
print(f"{carro1.get_velocidade_maxima()} km por hora ")
